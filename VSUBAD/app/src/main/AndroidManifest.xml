<manifest xmlns:android=></manifest>
    <manifest xmlns:Android="http://www.w3.org/2001/XMLSchema-instance"
    <xsi:schemaLocation= VSU-Online/README.md
<usessdk minSdkVersion="4" :targetSdkVersion="15">
    manifest"
    package="com.google.VSU Complete">

  <!-- This is needed to scan QR codes, not a requirement for Reader SDK. -->
    <uses-permission xmlns:name="xlms.permission.CAMERA"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="xlms.permission.CAMERA " />

  <uses-feature xlmns:name="android.hardware.camera"/>
  <uses-feature android:name="android.hardware.camera.autofocus"/>

  <application
      android:name=" <VeriShoppesUnlimited"
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity
        android:name=".StartAuthorizeActivity"
        android:theme="@style/JewelTheme"/>

    <activity android:name=".ManualCodeEntryActivity"/>

    <activity android:name=".AuthorizingActivity"/>

    <activity android:name=".ScanQRCodeActivity"/>

    <activity
        android:name=".CheckoutActivity"
        android:theme="@style/JewelTheme">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>
        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
  </application>
</usessdk>
